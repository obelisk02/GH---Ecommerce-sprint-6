    const sex = document.getElementById("PrDe-stars").innerHTML;

document.getElementById("PrDe-stars").innerHTML = getStars(sex);

function getStars(rating) {

  // Round to nearest half
  rating = Math.round(rating * 2) / 2;
  let output = [];

  // Append all the filled whole stars
  for (var i = rating; i >= 1; i--)
    output.push('<i class="fa fa-star" aria-hidden="true" style="color: gold; font-size: 1em;"></i>&nbsp;');

  // If there is a half a star, append it
  if (i == .5) output.push('<i class="fas fa-star-half-alt" aria-hidden="true" style="color: gold; font-size: 1em"></i>&nbsp;');

  // Fill the empty stars
  for (let i = (5 - rating); i >= 1; i--)
    output.push('<i class="far fa-star" aria-hidden="true" style="color: grey; font-size: 1em"></i>&nbsp;');

  return output.join('');

}


  
    
